public class SessionApp {

    static private Lote lote;

    static private PeixeDisponivel peixeDisponivel;

    static private List<Lote> lotes;

    static private Configuracoes configuracoes;

    static private Usuario usuario;

    public static List<Lote> getLotes() {
        return lotes;
    }

    public static void setLotes(List<Lote> lotes) {
        SessionApp.lotes = lotes;
    }

    public static Configuracoes getConfiguracoes() {
        return configuracoes;
    }

    public static void setConfiguracoes(Configuracoes configuracoes) {
        SessionApp.configuracoes = configuracoes;
    }

    public static Usuario getUsuario() {
        return usuario;
    }

    public static void setUsuario(Usuario usuario) {
        SessionApp.usuario = usuario;
    }

    public static Lote getLote() {
        return lote;
    }

    public static void setLote(Lote lote) {
        SessionApp.lote = lote;
    }

    public static PeixeDisponivel getPeixeDisponivel() {
        return peixeDisponivel;
    }

    public static void setPeixeDisponivel(PeixeDisponivel peixeDisponivel) {
        SessionApp.peixeDisponivel = peixeDisponivel;
    }
}
